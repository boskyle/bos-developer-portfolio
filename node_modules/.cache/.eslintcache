ection>\n\n            <h2 className={css(styles.leftSide2)}>DESCRIPTION</h2>\n            <p className={css(styles.leftSide2)}>{description}</p>\n            <section   className={css(styles.leftSide3)} id=\"leftAnimation2\">\n            <h2 className=\"mt-3\">TECHNOLOGY USED</h2>\n            <div className=\"technologies-container\">     \n            {tUsed}\n            </div>\n            <div className=\"project-links w-75\">\n            <a  href={code_link} target=\"_blank\" style={{display: \"inline-block\"}} className=\"mr-2\"><h2>CODE</h2></a>\n            <a  href={demo_link} target=\"_blank\" style={{display: \"inline-block\"}} className=\"ml-2\"><h2>DEMO</h2></a>\n            </div>\n            </section>\n        </div>\n        <div className=\"flex-item p-2\">\n          \n            <section id=\"rightAnimation\" className={css(styles.rightSide)}>\n            <AwesomeSlider\n            className=\"slider-container mb-5\"\n            animation=\"openAnimation\"\n            bullets={true}\n            >\n            {imageLinks}\n            </AwesomeSlider>\n\n            </section>\n         \n            \n        </div>\n    </div>\n\n    </Modal>);\n\n}\n\nexport default ProjectModal;",{"ruleId":"60","severity":1,"message":"61","line":64,"column":9,"nodeType":"62","messageId":"63","endLine":64,"endColumn":88,"fix":"64"},{"ruleId":"60","severity":1,"message":"61","line":65,"column":9,"nodeType":"62","messageId":"63","endLine":65,"endColumn":71,"fix":"65"},{"ruleId":"60","severity":1,"message":"61","line":66,"column":9,"nodeType":"62","messageId":"63","endLine":66,"endColumn":62,"fix":"66"},{"ruleId":"67","severity":1,"message":"68","line":7,"column":10,"nodeType":"69","messageId":"70","endLine":7,"endColumn":24},{"ruleId":"71","severity":1,"message":"72","line":76,"column":7,"nodeType":"73","endLine":76,"endColumn":9,"suggestions":"74"},{"ruleId":"60","severity":1,"message":"61","line":114,"column":13,"nodeType":"62","messageId":"63","end